{
  "name": "@omega/web",
  "version": "1.0.0",
  "description": "Omega Web Application - Development Cycle & Roadmap Dashboard",
  "author": "Prewave Team",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "webpack serve --config webpack.config.mjs --mode development",
    "start": "npm run dev",
    "build": "webpack --config webpack.config.mjs --mode production",
    "preview": "webpack serve --config webpack.config.mjs --mode production",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "lint": "eslint . --ext .vue,.ts,.tsx,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit",
    "type-check:test": "tsc --noEmit --project tsconfig.test.json"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/runtime": "^7.28.4",
    "@omega/config": "file:../../packages/config",
    "@omega/types": "file:../../packages/types",
    "@omega/utils": "file:../../packages/utils",
    "@vueuse/core": "^10.11.1",
    "@vueuse/shared": "^10.11.1",
    "ant-design-vue": "^4.2.6",
    "apexcharts": "^4.0.0",
    "babel-code-frame": "^6.26.0",
    "invariant": "^2.2.4",
    "vue": "^3.4.15",
    "vue-demi": "^0.14.10",
    "vue-router": "^4.2.5",
    "vue3-apexcharts": "^1.4.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.0.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^6.8.1",
    "eslint": "^9.0.0",
    "eslint-plugin-vue": "^9.19.2",
    "html-webpack-plugin": "^5.5.3",
    "jsdom": "^27.0.0",
    "postcss": "^8.5.6",
    "postcss-loader": "^7.3.3",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4",
    "vue-loader": "^17.4.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "overrides": {
    "vue-demi": "^0.14.0",
    "@vueuse/core": "^10.7.0",
    "@vueuse/shared": "^10.7.0"
