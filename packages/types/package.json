{
  "name": "@headnorth/types",
  "version": "1.0.0",
  "description": "Shared TypeScript types for Head North applications",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "build": "node esbuild.config.js && tsc --project tsconfig.declaration.json",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "lint": "eslint . --fix",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "esbuild": "^0.27.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "types",
    "headnorth",
    "shared",
    "typescript"
  ],
  "author": "Michael Wagner-Kulovits",
  "license": "MIT"
}
