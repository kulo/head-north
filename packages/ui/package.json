{
  "name": "@omega/ui",
  "version": "1.0.0",
  "description": "Shared UI components for Omega applications",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "build": "node esbuild.config.js && tsc --project tsconfig.declaration.json",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest",
    "lint": "eslint . --ext .ts,.js,.vue --fix --ignore-path .gitignore",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "esbuild": "^0.25.10",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "ui",
    "components",
    "omega",
    "shared",
    "typescript",
    "vue"
  ],
  "author": "Prewave Team",
  "license": "UNLICENSED"
}
